#include <stdio.h>

int main() {
    
    int n,curF=0,lastF=1,sum=0;
    
    printf("Input number of terms to display: ");
    scanf("%d", &n);

    
    for(int i=0; i<n; i++) {
      printf("%d ", sum);
      sum = curF + lastF;
      lastF = curF;
      curF = sum;
        
    }
    return 0;
}
